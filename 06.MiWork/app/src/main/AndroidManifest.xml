<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.developer.miwork">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Colors"
            android:label="Colors"/>
        <activity android:name=".Family"
            android:label="Family"/>
        <activity android:name=".Numbers"
            android:label="Numbers"/>
        <activity android:name=".Phrase"
            android:label="Phrase"></activity>
    </application>

</manifest>